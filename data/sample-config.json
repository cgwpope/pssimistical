{
    "tables": [
        {
            "columns": [
                {
                    "name": "name",
                    "type": "text"
                },
                {
                    "name": "id",
                    "type": "number"
                }
            ],
            "name": "patients"
        },
        {
            "columns": [
                {
                    "name": "Date",
                    "type": "number"
                },
                {
                    "name": "Date_Overidden",
                    "type": "text"
                }
            ],
            "name": "security_override_log"
        },
        {
            "columns": [
                {
                    "name": "Date",
                    "type": "text"
                },
                {
                    "name": "Booking_Date",
                    "type": "text"
                }
            ],
            "name": "appointments"
        }
    ],
    "readers": {
        "pss_patients_export": {
            "type": "SV",
            "readerProperties": {
                "linesToSkip": 2,
                "delimiter": ","
            },
            "columns": [
                "name",
                "Sex",
                "Age",
                "Birthdate",
                "doctor_name",
                "id",
                "Health_Num",
                "City",
                "Address",
                "Phone_Number",
                "Status",
                "Last_Billed_date",
                "?"
            ]
        },
        "security_override_log_export": {
            "type": "txlog",
            "readerProperties": {
                "recordSeparator": "Security Override Log Entry"
            },
            "columns": [
                "Patient Number",
                "Overridden by user",
                "Date Overidden",
                "Overriders IPAddress",
                "Override Type"
            ]
        },
        "appointments": {
            "type": "SV",
            "readerProperties": {
                "linesToSkip": 3,
                "delimiter": "\t"
            },
            "columns": [
                "date",
                "booking_date",
                "Booking Time",
                "Patient #",
                "Provider ID",
                "Provider Name",
                "Time",
                "Duration",
                "Details",
                "Status",
                "No Show",
                "LWBS",
                "Billed",
                "Repeat",
                "Deleted",
                "ForeColour",
                "BackColour",
                "Booking Initials",
                "Comments",
                "Alert",
                "Standby Priority"
            ]
        }
    },
    "inputs": [
        {
            "path": "data/patients.csv.txt",
            "table": "patients",
            "reader": "pss_patients_export"
        },
        {
            "path": "data/Transactions.txt",
            "table": "security_override_log",
            "reader": "security_override_log_export"
        },
        {
            "path": "data/appts.txt",
            "table": "appointments",
            "reader": "appointments"
        }
    ],
    "query": "SELECT count(*), name INTO CSV(\"output.csv\", {headers:true}) FROM patients group by name"
}